{
  "name": "name",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Andrew Johnston",
  "license": "MIT",
  "description": "A react boilerplate using Next and Express",
  "dependencies": {
    "address": "^1.1.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "lru-cache": "^5.1.1",
    "next": "^9.0.2",
    "npm": "^6.10.1",
    "prop-types": "^15.7.2",
    "raw-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-async-script": "^1.1.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url": "^0.11.0",
    "webpack": "^4.36.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "standard": "^13.0.2"
  },
  "scripts": {
    "test": "NODE_ENV=test jest",
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "deploy": "svn update && npm install && npm update && npm run build && pm2 restart www",
    "lint": "prettier 'utils/**/*.js' 'static/js/**/*.js' '__tests__/**/*.js' 'components/**/*.js' 'pages/**/*.js' '*.js' --write --single-quote --no-semi && standard --fix",
    "svn-stage": "svn status | grep '?' | sed 's/^.* /svn add /' | bash && svn status | grep '!' | sed 's/^.* /svn delete /' | bash",
    "stage": "npm run lint && npm run test && npm run svn-stage"
  },
  "standard": {
    "globals": [
      "$"
    ],
    "parser": "babel-eslint"
  }
}
